<h1>Listing user_infos</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Imgurl</th>
    <th>Phone</th>
    <th>Qq</th>
    <th>Stuno</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_infos.each do |user_info| %>
  <tr>
    <td><%= user_info.name %></td>
    <td><%= user_info.email %></td>
    <td><%= user_info.imgUrl %></td>
    <td><%= user_info.phone %></td>
    <td><%= user_info.qq %></td>
    <td><%= user_info.stuNo %></td>
    <td><%= link_to 'Show', user_info %></td>
    <td><%= link_to 'Edit', edit_user_info_path(user_info) %></td>
    <td><%= link_to 'Destroy', user_info, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

